{
  "author": "Steamship",
  "entrypoint": "api.handler",
  "handle": "audio-to-text-blockifier-default",
  "description": "Converts audio into Steamship's standard block format via Assembly AI",
  "public": true,
  "type": "plugin",
  "version": "1.0.0",
  "plugin": {
    "isTrainable": false,
    "transport": "jsonOverHttp",
    "type": "blockifier"
  },
  "configTemplate": {
    "speaker_detection": {
      "type": "boolean",
      "description": "Enable speaker detection.",
      "default": true
    },
    "enable_audio_intelligence": {
      "type": "boolean",
      "description": "Enable audio intelligence.",
      "default": true
    }
  }
}